export type InitState = {
  isLoading: boolean
  isLoggedIn: boolean
}

export const initState: InitState = {
  isLoading: false,
  isLoggedIn: false
}

export const <%= reducerName %>Reducer = (state: InitState, action: any) => {
  const { payload, type } = action

  switch(type) {
    case 'loading': {
      const { loading } = payload
      return {
        ...state,
        isLoading: loading
      }
    }
    case 'login': {
      return {
        ...state,
        isLoggedIn: true
      }
    }
    case 'logout': {
      return {
        ...state,
        isLoggedIn: false
      }
    }
    default: 
      return state
  }
}
